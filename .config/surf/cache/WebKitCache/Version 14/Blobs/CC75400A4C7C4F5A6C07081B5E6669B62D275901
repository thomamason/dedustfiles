              <div prefix="" slot="prefix" class="style-scope ytd-donation-amount-picker-renderer">[[getSimpleString(data.currencySymbol)]]&nbsp;</div>
            </paper-input>
          </div>
        </div>

        <div id="button-row" class="style-scope ytd-donation-amount-picker-renderer">
          <ytd-button-renderer data="[[data.donateButton.buttonRenderer]]" on-tap="onDonateButtonTap" class="style-scope ytd-donation-amount-picker-renderer">
